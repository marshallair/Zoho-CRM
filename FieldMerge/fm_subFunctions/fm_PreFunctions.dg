string standalone.fmfunc_IF(string strIndex)
{
Index=strIndex.toList();
m=map();
ResultMap=map();
ResultMap.put("blocktype","IF");
m.put(standalone.Index.add(0).duplicate(0).toList(),"IF_COND");
m.put(standalone.Index.add(1).duplicate(0).toList(),"IF_TRUE");
m.put(standalone.Index.add(2),duplicate(0).toList(),"IF_FALSE");
ResultMap.put("preload_blocks",m);
Return ResultMap;
}
