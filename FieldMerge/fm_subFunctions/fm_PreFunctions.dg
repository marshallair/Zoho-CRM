string standalone.fmfunc_FIELD(string strIndex)
{
//TEST - result should be "{"blocktype":"FIELD","preload_blocks":{[2,4,0]:"FIELD_NAME",[2,4,1]:"FIELD_NOT_NULL",[2,4,2]:"FIELD_NULL",[2,4,3]:"FIELD_ERROR"}}"
// strIndex=[2,4];
i=strIndex.toList(",");
m=map();
ResultMap=map();
ResultMap.put("blocktype","FIELD");
i.add(0);
m.put(i.duplicate(0).toList(),"FIELD_NAME");
i=standalone.IncrementSequentialIndex(i).toList(",");
m.put(i.duplicate(0).toList(),"FIELD_NOT_NULL");
i=standalone.IncrementSequentialIndex(i).toList(",");
m.put(i.duplicate(0).toList(),"FIELD_NULL");
i=standalone.IncrementSequentialIndex(i).toList(",");
m.put(i.duplicate(0).toList(),"FIELD_ERROR");
ResultMap.put("preload_blocks",m);Return ResultMap;
}
